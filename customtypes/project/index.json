{
  "format": "page",
  "id": "project",
  "label": "Project",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "Project title"
        }
      },
      "client": {
        "type": "Text",
        "config": {
          "label": "Client",
          "placeholder": "Self-initiated"
        }
      },
      "year": {
        "type": "Number",
        "config": {
          "label": "Year",
          "placeholder": "2024"
        }
      },
      "role": {
        "type": "Text",
        "config": {
          "label": "Role",
          "placeholder": "Typeface design, Web dev"
        }
      },
      "external_url": {
        "type": "Link",
        "config": {
          "label": "External Url",
          "placeholder": "https://anassageneral.gr",
          "allowTargetBlank": true,
          "select": null
        }
      },
      "heading": {
        "type": "Text",
        "config": {
          "label": "Heading",
          "placeholder": "Air Company, a carbon technology company that is creating products from CO2 to extend life on Earth."
        }
      },
      "description": {
        "type": "Text",
        "config": {
          "label": "Description",
          "placeholder": ""
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "video_showcase": {
              "type": "SharedSlice"
            },
            "media_showcase": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "slices1": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    }
  }
}
